    "@types/storybook__addon-knobs": "^5.0.2",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "autoprefixer": "^9.6.1",
    "axios-mock-adapter": "^1.17.0",
    "babel-plugin-react-docgen-typescript": "^1.2.0",
