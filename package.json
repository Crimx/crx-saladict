    "react-hot-loader": "^4",
    "react-i18next": "^10.11.4",
    "react-shadow": "^17.1.1",
    "react-transition-group": "^4.2.1",
    "rxjs": "^6.5.2",
    "web-ext-types": "^3.2.1",
    "webextension-polyfill": "latest"
